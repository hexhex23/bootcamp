<p>
  <a href="/viewall">View All Courses/  </a>
  <a href="/addcourse">Add Course/  </a>
  <a href="/admin/adprofile">Profile/  </a>
  <a href="/admin/logout">Logout</a>
</p>

<h3>View all Existing Courses Here</h3>
<table class='table table-hover table-responsive table-bordered'>
    <td>Provider Name</td>
    <td>Course Title</td>
    <td>Subject Area</td>
    <td>Delivery Mode</td>
    <td>Format</td>
    <td>Length</td>
    <td>Experience Level</td>
    <td>Cost</td>
    <td>Subsidy Availability</td>
    <td>Subsidies</td>
    <td>Intakes</td>
    <td>Some Topics</td>
    <td>Great For</td>
    <td>Be Aware</td>
    <td>Website</td>
    <td></td>

    <% course.forEach(function(course, index){ %>
      <div class='course-container'>
        <p>
    <tr>
    <td><%=course.provider%></td>
    <td><%=course.title%></td>
    <td><%=course.subjectarea%></td>
    <td><%=course.deliverymode%></td>
    <td><%=course.format%></td>
    <td><%=course.length%></td>
    <td><%=course.experiencelevel%></td>
    <td><%=course.price%></td>
    <td><%=course.subsidyavailability%></td>
    <td><%=course.subsidies%></td>
    <td><%=course.intakes%></td>
    <td><%=course.sometopics%></td>
    <td><%=course.great%></td>
    <td><%=course.aware%></td>
    <td><%=course.website%></td>
    <td><form id='show_edit' action =<%= '/update/' + course._id %> method='get'>
          <input type="submit" value="Edit This Record" />
      </form>
      <p></p>
      <form id='show_delete' action =<%= '/delete/' + course._id %> method='get'>
            <input type="submit" value="Delete This Record" />
        </form>
      </td>
    </tr>

    </p>
    </div>

    <% }) %>

      </table>
